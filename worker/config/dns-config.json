{
  "version": "3.0.0",
  "worker_url": "https://dns-tunnel.hallochrrr.workers.dev",
  "endpoints": {
    "dns_tunnel": "/dns-tunnel?action=connect",
    "dns_query": "/dns-query?domain=example.com",
    "tunnel_data": "/tunnel",
    "socks_config": "/socks",
    "status": "/status",
    "http_proxy": "/proxy"
  },
  "protocols": {
    "primary": "dns-over-https",
    "fallback": "http-tunnel",
    "encryption": "aes-256-gcm"
  },
  "client_settings": {
    "timeout": 30,
    "retry_attempts": 5,
    "compression": true,
    "chunk_size": 512
  }
}
