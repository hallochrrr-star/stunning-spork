{
  "version": "2.0.0",
  "worker_url": "https://dns-tunnel.hallochrrr.workers.dev",
  "endpoints": {
    "dns_query": "/dns-query",
    "tunnel": "/tunnel",
    "socks": "/socks",
    "status": "/status"
  },
  "dns_settings": {
    "timeout": 30000,
    "retry_attempts": 3,
    "compression": true,
    "encryption": true,
    "chunk_size": 512
  },
  "tunnel_settings": {
    "local_port": 1080,
    "socks_version": 5,
    "auto_reconnect": true
  }
}
